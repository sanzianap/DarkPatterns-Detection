{
  "manifest_version": 3,
  "name": "Dark patterns recognition",
  "version": "1.0",
  "description": "Detects and highlights dark patterns in he cookie modal",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "dist/content.bundle.js"
      ],
      "matches": [
        "https://*/*",
        "https://*/"
      ]
    }
  ]
}